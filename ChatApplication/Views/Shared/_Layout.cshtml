        <div class="container text-light">
            &copy; 2020 - ChatApplication - <a asp-area="" asp-controller="Home" asp-action="Privacy" class="btn btn-outline-light">Privacy</a>
            &nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;
            <span class="text-dark p-3 rounded-circle" style="background-color:white;font-size:1.3vw;">
                <b>Contact-no:9876543210 &nbsp; &nbsp; Email-id:ChatHN@gmail.com</b>
            </span>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
